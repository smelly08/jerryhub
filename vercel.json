{
  "version": 2,
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "^/$",
      "dest": "/index.html"
    },
    {
      "src": "^/profile/?$",
      "dest": "/profile/profile.html"
    },
    {
      "src": "^/bazaar/?$",
      "dest": "/bazaar/bazaar.html"
    },
    {
      "src": "^/extra/?$",
      "dest": "/extra/extra.html"
    }
  ]
}
